    "#斐波那契问题\n",
    "class Solution:\n",
    "    def climbStairs(self, n: int) -> int:\n",
    "        fn=[1,1,1]\n",
    "        for _ in range(1,n):\n",
    "            fn[2]=fn[1]+fn[0]\n",
    "            fn[1],fn[0]=fn[2],fn[1]# 这个交换是python相比其他语言最得天独厚的优势\n",
    "        return fn[2]"
   ]
  }
 ],
