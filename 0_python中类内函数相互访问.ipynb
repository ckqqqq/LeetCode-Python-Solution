    "class Animal(object):\n",
    "    def __init__(self,name):\n",
    "        self.name=name\n",
    " \n",
    "    def eat(self,food):\n",
    "        print(f'{self.name}：正在吃{food}')\n",
    " \n",
    "    def sleep(self):\n",
    "        self.eat('狗粮')\n",
    "        print(f'{self.name}：正在睡觉')\n",
    " \n",
    "dog=Animal('大黄')\n",
    "dog.sleep()"
   ]
  }
