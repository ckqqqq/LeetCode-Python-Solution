    "# 统计1的个数\n",
    "class Solution:\n",
    "    def hammingWeight(self, n: int) -> int:\n",
    "        count=0\n",
    "        while n!=0:\n",
    "            n=n&n-1#去除最后一个一\n",
    "            #n&-n 获取最后一个一\n",
    "            count+=1\n",
    "        return count"
   ]
